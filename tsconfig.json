{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "removeComments": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "preserveConstEnums": true,
        "outDir": "dist",
        "jsx": "preserve"
    },
    "exclude": [
        "node_modules"
    ]
}